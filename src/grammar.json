{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "lua_patterns",
  "rules": {
    "pattern": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "start_assertion"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "SYMBOL",
                "name": "literal_character"
              },
              {
                "type": "SYMBOL",
                "name": "character_class"
              },
              {
                "type": "SYMBOL",
                "name": "character_set"
              },
              {
                "type": "SYMBOL",
                "name": "escaped_character"
              },
              {
                "type": "SYMBOL",
                "name": "capture_group"
              },
              {
                "type": "SYMBOL",
                "name": "any_character"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "end_assertion"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "start_assertion": {
      "type": "STRING",
      "value": "^"
    },
    "end_assertion": {
      "type": "STRING",
      "value": "$"
    },
    "zero_or_more": {
      "type": "PATTERN",
      "value": "\\*"
    },
    "one_or_more": {
      "type": "PATTERN",
      "value": "\\+"
    },
    "lazy": {
      "type": "PATTERN",
      "value": "-"
    },
    "optional": {
      "type": "PATTERN",
      "value": "\\?"
    },
    "literal_character": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "[^\\(\\[\\.]"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "zero_or_more"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "one_or_more"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "lazy"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "optional"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "any_character": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "zero_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "one_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "lazy"
                },
                {
                  "type": "SYMBOL",
                  "name": "optional"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "character_class": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "PATTERN",
          "value": "[acdlpsuwxzACDLPSUWXZ]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "zero_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "one_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "lazy"
                },
                {
                  "type": "SYMBOL",
                  "name": "optional"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "character_range": {
      "type": "SEQ",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "set_character"
          },
          "named": true,
          "value": "literal_character"
        },
        {
          "type": "STRING",
          "value": "-"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "set_character"
          },
          "named": true,
          "value": "literal_character"
        }
      ]
    },
    "escaped_character": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "PATTERN",
          "value": "[^acdlpsuwxzACDLPSUWXZ]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "zero_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "one_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "lazy"
                },
                {
                  "type": "SYMBOL",
                  "name": "optional"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "escape_sequence": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\\\[bfntvr0'\"\\\\]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "zero_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "one_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "lazy"
                },
                {
                  "type": "SYMBOL",
                  "name": "optional"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "set_sequence": {
      "type": "PATTERN",
      "value": "\\\\[bfntvr0'\"\\\\]"
    },
    "set_class": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "PATTERN",
          "value": "[acdlpsuwxzACDLPSUWXZ]"
        }
      ]
    },
    "set_character": {
      "type": "PATTERN",
      "value": "[^\\^\\$\\+\\*\\-\\?\\(\\)\\[\\]\\.]"
    },
    "set_escaped": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "PATTERN",
          "value": "[^acdlpsuwxzACDLPSUWXZ]"
        }
      ]
    },
    "set_any": {
      "type": "STRING",
      "value": "."
    },
    "character_set_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "character_range"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "set_sequence"
            },
            "named": true,
            "value": "escape_sequence"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "set_class"
            },
            "named": true,
            "value": "character_class"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "set_character"
            },
            "named": true,
            "value": "literal_character"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "set_escaped"
            },
            "named": true,
            "value": "escaped_character"
          },
          {
            "type": "ALIAS",
            "content": {
              "type": "SYMBOL",
              "name": "set_any"
            },
            "named": true,
            "value": "any_character"
          }
        ]
      }
    },
    "character_set": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "^"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "character_set_content"
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "zero_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "one_or_more"
                },
                {
                  "type": "SYMBOL",
                  "name": "lazy"
                },
                {
                  "type": "SYMBOL",
                  "name": "optional"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "capture_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "character_set"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "set_sequence"
                },
                "named": true,
                "value": "escape_sequence"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "set_class"
                },
                "named": true,
                "value": "character_class"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "set_character"
                },
                "named": true,
                "value": "literal_character"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "set_escaped"
                },
                "named": true,
                "value": "escaped_character"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "set_any"
                },
                "named": true,
                "value": "any_character"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
